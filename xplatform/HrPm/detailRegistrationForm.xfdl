	ds_salInfo.filter("EMP_CODE=='"+selEmpCode+"'");
	ds_salInfo.rowposition=0;
	
	ds_position.filter("POSITION_CODE=='"+selPositionCode+"'");
	ds_position.rowposition=0;
	
	ds_familyInfo.filter("EMP_CODE=='"+selEmpCode+"'");
	ds_familyInfo.rowposition=0;
	
	ds_licenseInfo.filter("EMP_CODE=='"+selEmpCode+"'");
	ds_licenseInfo.rowposition=0;
	
	ds_educationInfo.filter("EMP_CODE=='"+selEmpCode+"'");
	ds_educationInfo.rowposition=0;
	
	
	
	ds_hobong.filter("POSITION_CODE=='"+selPositionCode+"'");
	ds_hobong.rowposition=0;
	ds_hobongCopy.copyData(ds_hobong,true); //직급이 가진 호봉만 담기 위해!
	ds_hobongCopy.rowposition=0;
		
	ds_hobong.filter("POSITION_CODE=='"+selPositionCode+"' && HOBONG=='"+ds_salInfo.getColumn(0,"HOBONG")+"'");
	ds_hobong.rowposition=0;

	
	if(Tab00.tabpage3.selHobong.value!=null){
		ds_hobong.filter("POSITION_CODE=='"+ds_position.getColumn(0,"POSITION_CODE")+"' && HOBONG=='"+Tab00.tabpage3.selHobong.value+"'");

		Tab00.tabpage3.baseSal.value=ds_hobong.getColumn(0,"BASE_SAL");
		Tab00.tabpage3.bonus.value=ds_hobong.getColumn(0,"BONUS");
		Tab00.tabpage3.sigup.value=ds_hobong.getColumn(0,"SIGUB");
	}else{
	    
		Tab00.tabpage3.baseSal.value="";
		Tab00.tabpage3.bonus.value="";
		Tab00.tabpage3.sigup.value="";
	}
	
	
	

	
	var empImgName=ds_empInfo.getColumn(0,'IMG_SRC');
	if(empImgName!=null){
		Tab00.tabpage1.empImg.image="http://localhost:8282/test5th/img/"+empImgName;	
		Tab00.tabpage1.empImg.stretch="fit"; 
	}else{
		Tab00.tabpage1.empImg.image="http://localhost:8282/test5th/img/noimage.jpg";
		Tab00.tabpage1.empImg.stretch="fit"; 
	}	
		//empImg.ImageID="http://localhost/xp5/EMP_IMG/"+empImgName+"?time="+timem();   
	
	
}


