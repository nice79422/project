function Grid00_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	selEmpCode = ds_employeeList.getColumn(e.row,"EMP_CODE");
	selPositionCode = ds_employeeList.getColumn(e.row,"POSITION_CODE"); // 선택한 사원의 직급을 담음 
	
	ds_empInfo.filter("EMP_CODE=='"+selEmpCode+"'");
