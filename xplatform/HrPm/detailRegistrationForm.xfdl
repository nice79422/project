function callback(trid, ErrorCode, ErrorMsg) {
		
	if(trid=='batchEmployeeInfoAll'){
		if(ErrorCode!=0){
			alert(ErrorMsg);
		}else{
			alert(" 성공적으로 데이터를 저장하였습니다 :) ");
			reload();
		}	
	}
	
}



	



	

