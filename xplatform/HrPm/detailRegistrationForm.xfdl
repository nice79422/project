    <Script type="xscript4.0"><![CDATA[
var selEmpCode;
var selPositionCode;

var imgDialog;

var empFileName;


// 로딩시 
function detailRegistrationForm_onload(obj:Form, e:LoadEventInfo)
{
	//사원목록 
	ds_employeeList.filter("");
	//부서목록 - 다른곳 안들러도 로그인 시 가져옴
	ds_dept.filter("");
	//직급목록 - 로그인시 안가져옴 새로 가져와야됨
	ds_position.filter("");
	//호봉목록 - 수정되었을 경우 있기 때문에 새로 얻어옴 
	ds_hobong.filter("");
	
	ds_salInfo.filter("");

	//사원목록 구해오기
	 transaction("findEmployeeList",
				"svcBase::findEmployeeList.do",
				"",
				"ds_employeeList=dsEmployee",
				"",
				"callback",
				false);	

	
	//직급목록 구해오기 
	transaction("findPositionList",
				"svcBase::findPositionList.do",
				"",
				"ds_position=dsPosition",
				"",
				"callback",
				false);
				
	//코드정보 구해 오기 
	transaction("findCodeList",
				"svcBase::findCodeList.do",
				"",
				"ds_code=dsCode ds_detailCode=dsDetailCode",
				"",
				"callback",
				false);
	
 	//호봉 얻어 오기 
	transaction("findHobongList",
 				"svcHrCircumstance::findHobongList.do",
				"",
 				"ds_hobong=dsHobong",
				"",
 				"callback",
 				false);
 				

	//4대 보험 & 소득세 얻어옴 !!
	ds_deductionTax.filter("");
	ds_incomeTax.filter("");
	transaction("findDeductionsInsuranceList",
				"svcHrCircumstance::findDeductionInsuranceList.do",
				"",
				"ds_deductionTax=dsDeductionTax ds_incomeTax=dsIncomeTax",
				"",
				"callback",
				false);
	
	//6개 상세 정보 구하기
	transaction("findEmployeeInfoAll",
		"svcHrPM::findEmployeeInfoAll.do",
		"",
		"ds_empInfo=dsEmpInfo ds_familyInfo=dsFamilyInfo ds_licenseInfo=dsLicenseInfo ds_workInfo=dsWorkInfo ds_salInfo=dsSalInfo ds_educationInfo=dsEducationInfo",
		"",
		"callback",
		false
	);
	
	//trace(dsEmpInfo.getRowCount()+"@@"+dsFamilyInfo.getRowCount()+"@@"+dsLicenseInfo.getRowCount());
	//trace(dsWorkInfo.getRowCount()+"@@"+dsSalInfo.getRowCount()+"@@"+dsEducationInfo.getRowCount());
	//초기 상태
	
	
	ds_empInfo.filter("EMP_CODE==0"); //처음에 선택된 내용 없음!
	ds_workInfo.filter("EMP_CODE==0");
	ds_salInfo.filter("EMP_CODE==0");
	ds_familyInfo.filter("EMP_CODE==0");
	ds_licenseInfo.filter("EMP_CODE==0");
	ds_educationInfo.filter("EMP_CODE==0");
	
	ds_position.filter("POSITION_CODE==0");
	ds_hobongCopy.filter("POSITION_CODE==0");
	
	
	
	
	detailCodeFilter();//코드 설정하기 
	
	
}

//코드 설정
function detailCodeFilter(){
	//내국인여부
	ds_detailCode.filter("String(DETAIL_CODE).substr(0,2) == 'NA'");
	ds_nativeType.copyData(ds_detailCode,true);
	
	//병역사항
	ds_detailCode.filter("String(DETAIL_CODE).substr(0,2) == 'DI'");
	ds_disabilityType.copyData(ds_detailCode,true);
	
	//고용형태
	ds_detailCode.filter("String(DETAIL_CODE).substr(0,3) == 'EMT'");
	ds_employeementType.copyData(ds_detailCode,true);
	
	//직종
	ds_detailCode.filter("String(DETAIL_CODE).substr(0,3) == 'JJC'");
	ds_jikjong.copyData(ds_detailCode,true);
	
	//프로젝트
	ds_detailCode.filter("String(DETAIL_CODE).substr(0,3) == 'PJC'");
	ds_project.copyData(ds_detailCode,true);
	
	//급여형태 
	ds_detailCode.filter("String(DETAIL_CODE).substr(0,3) == 'PAC'");
	ds_salaryType.copyData(ds_detailCode,true);
	
	//가족관계
	ds_detailCode.filter("String(DETAIL_CODE).substr(0,3) == 'RLC'");
	ds_relationship.copyData(ds_detailCode,true);
	
	//최종학력
	ds_detailCode.filter("String(DETAIL_CODE).substr(0,3) == 'EDC'");
	ds_highestEducation.copyData(ds_detailCode,true);
	
	//학교선택
	ds_detailCode.filter("String(DETAIL_CODE).substr(0,3) == 'UNC'");
	ds_university.copyData(ds_detailCode,true);

}



//부서 선택시 
function deptSerchCombo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	
	ds_employeeList.filter("DEPT_CODE=='"+deptSerchCombo.value+"'");
	ds_tempEmp.copyData(ds_employeeList,true); // filter가 된 사원 리스트를 띄운다.
}


//사진 찾기
function Tab00_tabpage1_findPicture_onclick(obj:Button,  e:ClickEventInfo)
{
	
	imgDialog=FileDialog.open("FileOpen",FileDialog.Load);
	//alert(imgDialog);  // object.VirtualFiel이 나온다..
	imgName = imgDialog.filename;
	imgPath = imgDialog.path;
	alert(imgPath+"@@@@@@"+imgName);
	img = imgPath+imgName;
	Tab00.tabpage1.empImg.image=img;
	Tab00.tabpage1.empImg.stretch="fit";
	
	
	
	
}
//사진저장시!
function Tab00_tabpage1_savePicture_onclick(obj:Button,  e:ClickEventInfo)
{


	var fileExtend = String(imgName).substr(imgName.lastIndexOf('.'));
	   
	empFileName =  selEmpCode + fileExtend;
	alert(empFileName +"::::::::::::"+selEmpCode);
	
	
	imgDialog.open(VirtualFile.openRead | VirtualFile.openBinary); // 앞쪽에서 선언함.. imgDialog = virtualfile 을 return  F1 -> fileDialog에서 반환받은 경우!
				//읽기전용파일 열때 |  바이러리파일을 열때 사용
	//alert(imgDialog.open(VirtualFile.openRead | VirtualFile.openBinary)); //true,false
	var img_binary=imgDialog.read(imgDialog.getFileSize());
	//버퍼데이터 또는 문자열 반환 
	
		
		// 데이터셋에 추가하기 전에 기존 데이터가 있으면 삭제한다.
	var findNum = ds_img.findRow("EMP_FILE_NAME",empFileName);
	if(findNum > -1){
		ds_img.deleteRow(findNum); 
		trace("기존dataset은 지워 버린다.");
	}
	
	ds_img.addRow();
	ds_img.setColumn(0,"EMP_FILE_NAME",empFileName);
	ds_img.setColumn(0,"IMG_FILE_DATA",img_binary);
	
	//이미지 주소 담음
	ds_empInfo.setColumn(ds_empInfo.rowposition,"IMG_SRC",empFileName);

	trace(ds_img.getRowCount()+"@@@@@@@@@@@@");
	
	
	transaction("saveEmpImg",
				"svcHrPM::saveEmpImg.do",
				"dsImg=ds_img:u",
				"",
				"",
				"callback",
				false);
	
	
}


